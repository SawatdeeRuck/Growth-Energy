					.articletitle { } 
					.matched { } 
					.articleComment { } 
					.topMenuSub { } 
					.categorylink { } 
					.categorylink a:link { } 
					.categorylink a:visited { } 
					.categorylink a:hover { } 
					.categoryname { } 
					.date { } 
					.source { } 
					.articletitle a:link { } 
					.articletitle a:visited { } 
					.articletitle a:hover { } 
					a:link { } 
					a:visited { } 
					a:hover { } 
					.more a:link { } 
					.more a:visited { } 
					.more a:hover { } 
					@media screen and (max-width: 630px){
					*[class="mobile-column"] {width: 100% !important;} 
					</style> 
			</head> 
			<body class="body" style="background-color: #DBDBDB; margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-left: 0px; padding-top: 0px; padding-right:0px;"> 
				<xsl:comment></xsl:comment>
				<center> 
					<table cellspacing="0" cellpadding="0" border="0" bgcolor="#ffffff" class="mobile-column" style="border: #DBDBDB solid 0px; margin-top: 0px; margin-right: 0px; margin-left: 0px; margin-bottom: 0px; padding: 0px; width: 660px;"> 
						<tr> 
							<td bgcolor="#ffffff"> 

						        <div style="display: none; font-size: 1px; line-height: 1px; max-height: 0px; max-width: 0px; opacity: 0; overflow: hidden;">
						          Read the latest news that affects the ethanol industry, courtesy of Growth Energy.
						        </div>

								<table border="0" cellpadding="0" cellspacing="0" width="100%" align="left" height="50" style="background-color:#ffffff;" bgcolor="#ffffff">
									<tr>
										<td height="20" bgcolor="#DBDBDB"> </td>
									</tr> 

									<tr valign="bottom"> 
										<td align="left" valign="top" bgcolor="#DBDBDB" style="padding: 0px;"> 
											<a href="http://www.growthenergy.org/" target="_blank"> 
												<img src="https://raw.githubusercontent.com/SawatdeeRuck/Growth-Energy/429f94742248580f11551ef3cc024370fbdfe2aa/images/clips-header.png" border="0" width="100%" alt="growthenergy" /></a> 
											</td> 
										</tr> 
									</table> 
								</td></tr> 
								<tr><td height="25" align="left" style="padding: 20px 20px 0px 20px;"><span class="matched" style="font-size: 15px; font-family: 'Roboto', Arial, sans-serif; color: #7AC142; font-weight: bold; line-height: 20px;">
									<!-- Date is Automatic -->

									<!--Friday, August 19, 2011-->
									<xsl:value-of select="datetime:month-name()" />
									<xsl:text> </xsl:text>
									<xsl:value-of select="datetime:day-in-month()" />
									<xsl:text disable-output-escaping="yes">&amp;nbsp;</xsl:text>

									<xsl:value-of select="datetime:year()" />
								</span></td></tr>

								<tr><td bgcolor="#ffffff"> 
									<table border="0" cellpadding="0" width="100%" cellspacing="0" align="left" bgcolor="#ffffff"> 
										<tr valign="top"><td align="left" valign="top" bgcolor="#ffffff"> 
											<table border="0" cellpadding="0" cellspacing="0" width="100%" bgcolor="#ffffff"> 
												<tr><td  bgcolor="#ffffff"> 
													<xsl:apply-templates/> 

												</td></tr> 
												<tr><td bgcolor="#ffffff" height="20"><xsl:text disable-output-escaping="yes"> &amp;nbsp;</xsl:text></td></tr> 
