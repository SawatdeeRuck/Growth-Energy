	<xsl:template match="/newsletter"> 
		<xsl:apply-templates select="summary"/> 
		<xsl:apply-templates select="category"/> 
		<xsl:apply-templates select="uncategorised"/> 
		<xsl:apply-templates select="endnote"/> 
	</xsl:template> 

	<xsl:template match="summary"> 
		<table cellspacing="0" cellpadding="0" border="0" width="100%" bgcolor="#ffffff"> 
			<tr> 
				<td bgcolor="#ffffff" style="padding: 5px 20px 0 20px;"><span class="matched" style="font-size: 14px; font-family: 'Roboto', Arial, sans-serif; line-height: 20px; color: #666666; font-weight: normal;"><xsl:value-of select="summary" disable-output-escaping="yes" /></span></td> 
			</tr> 
		</table> 
	</xsl:template> 

	<xsl:template match="endnote"> 
		<table cellspacing="0" cellpadding="0" border="0" width="100%" bgcolor="#ffffff"> 
			<tr> 
				<td bgcolor="#ffffff"><span class="matched" style="font-size: 12px; font-family: 'Roboto', Arial, sans-serif; color: #666666; font-weight: normal;"><xsl:value-of select="endnote" disable-output-escaping="yes" /></span></td> 
			</tr> 
		</table> 
	</xsl:template> 

	<xsl:template match="uncategorised"> 
		<xsl:if test="count(article) > 0"> 
			<table cellspacing="0" cellpadding="0" border="0" width="100%" bgcolor="#ffffff"> 

				<tr> 
					<td> 
						<xsl:for-each select="article"> 
							<xsl:apply-templates select="."/> 
							<table cellpadding="0" cellspacing="0" border="0"><tr><td height="30" bgcolor="#ffffff"><xsl:text disable-output-escaping="yes"> &amp;nbsp;</xsl:text></td></tr></table> 
						</xsl:for-each> 
					</td></tr> 

					<tr><td height="15" bgcolor="#ffffff"><xsl:text disable-output-escaping="yes"> &amp;nbsp;</xsl:text></td></tr> 

				</table> 
			</xsl:if> 
		</xsl:template> 

		<xsl:template match="category"> 
			<xsl:if test="count(article) > 0 or @isGadget = 'True'"> 
				<a> 
					<xsl:attribute name="id"><xsl:value-of select="@categoryId"/></xsl:attribute> 
					<xsl:attribute name="name"><xsl:value-of select="@categoryId"/></xsl:attribute> 
					<xsl:attribute name="isgadget"><xsl:value-of select="@isGadget"/></xsl:attribute> 
				</a> 

				<table cellspacing="0" cellpadding="0" border="0" width="100%" bgcolor="#ffffff"> 


					<tr><td height="15" bgcolor="#ffffff"><xsl:text disable-output-escaping="yes"> &amp;nbsp;</xsl:text></td></tr> 

					<tr><td bgcolor="#6DBB30" style="padding:10px 20px;"> 
						<span class="categoryname" style="font-size: 18px; color: #ffffff; font-family: 'Roboto', Arial, sans-serif; font-weight: normal; text-transform:uppercase;"><xsl:value-of select="@categoryName"/></span> 
					</td> 
				</tr> 

				<tr><td height="15" bgcolor="#ffffff"><xsl:text disable-output-escaping="yes"> &amp;nbsp;</xsl:text></td></tr> 


				<xsl:if test="count(article) > 0"> 
					<tr> 
						<td bgcolor="#ffffff" style="padding-left: 20px; padding-right: 20px;"> 
							<xsl:for-each select="article"> 
								<xsl:apply-templates select="."/> 

								<table cellpadding="0" cellspacing="0" border="0"><tr><td height="30" bgcolor="#ffffff"><xsl:text disable-output-escaping="yes"> &amp;nbsp;</xsl:text></td></tr></table> 

							</xsl:for-each> 
						</td></tr> 
					</xsl:if> 
					<xsl:if test="@isGadget = 'True'"> 
						<tr> 
							<td bgcolor="#ffffff"> 
								<xsl:if test="url != ''"> 
									<img> 
										<xsl:attribute name="src"><xsl:value-of select="url"/></xsl:attribute> 
									</img> 
								</xsl:if> 
								<xsl:if test="cloud != ''"> 
									<div style="padding: 0.43em; font-family: 'Roboto', Arial, sans-serif; text-align: center; font-size: 12px; color: #000000;"> 
										<xsl:value-of disable-output-escaping="yes" select="cloud" /> 
									</div> 
								</xsl:if> 
							</td></tr> 
						</xsl:if> 
					</table> 
				</xsl:if> 
			</xsl:template> 

			<xsl:template match="article"> 
				<table width="100%" cellspacing="0" cellpadding="0" border="0" bgcolor="#ffffff"> 
					<tr align="left" valign="top"> 
						<td bgcolor="#ffffff"> 

							<xsl:if test="(language = 'Arabic')"><xsl:attribute name="align">right</xsl:attribute></xsl:if> 
							<xsl:if test="language != 'Arabic'"><xsl:attribute name="align">left</xsl:attribute></xsl:if> 

							<div><a class="articletitle" style="font-size: 18px; font-family: 'Roboto', Arial, sans-serif; color: #666666; font-weight: bold; text-decoration: none; line-height: 24px;" OnMouseOver="this.style.textDecoration='underline'; this.style.color='#666666'" OnMouseOut="this.style.textDecoration='none';this.style.color='#666666'"> 
								<xsl:attribute name="href"><xsl:value-of select="url" /> 
							</xsl:attribute> 
							<xsl:attribute name="target">_blank</xsl:attribute> 
							<xsl:value-of select="title" disable-output-escaping="yes" /> 
						</a></div> 


						<div style="margin-top: 3px; margin-bottom: 3px;" ng-model="checked" ng-init="checked=true"> 
							<span class="source" style="border:1px solid #7AC142; padding:4px; font-size: 12px; font-family: 'Roboto', Arial, sans-serif; color: #666666; font-weight: normal; line-height: 25px;"> 


								<xsl:value-of select="month"/> 
								<xsl:text>.</xsl:text> 
								<xsl:value-of select="day"/> 
								<xsl:text>.</xsl:text> 
								<xsl:value-of select="year"/> </span> 
								<span class="source" style="font-size: 12px; font-family: 'Roboto', Arial, sans-serif; color: #666666; font-weight: normal; line-height: 20px;"> 
									<xsl:text disable-output-escaping="yes"> &amp;nbsp;</xsl:text>
									<xsl:value-of select="source" disable-output-escaping="yes" /> 

								</span> 
							</div> 

							<div class="ingress" style="font-size: 14px; font-family: 'Roboto', Arial, sans-serif; color: #666666; font-weight: normal; line-height: 20px; margin-top:5px; margin-bottom:5px;"><xsl:value-of select="ingress" disable-output-escaping="yes" /></div> 

							<!--xsl:choose-->
							<xsl:if test="(string-length(comment) > 0)">
							<div class="articleComment" style="font-size: 14px; font-family: 'Roboto', Arial, sans-serif; color: #666666; font-weight: normal; font-style: italic; line-height: 20px; margin-top: 10px;"><xsl:value-of select="comment" disable-output-escaping="yes"/></div> 
							</xsl:if>
							<!--xsl:otherwise>
							 
							</xsl:otherwise>
							</xsl:choose-->

							<div class="matched" style="font-size: 14px; font-family: 'Roboto', Arial, sans-serif; color: #666666; font-weight: normal; line-height: 20px; margin-top:5px; margin-bottom:15px; border: 1px solid #7AC142; padding:10px;"><xsl:value-of select="matchedSentences" disable-output-escaping="yes"/></div>


							<div><a class="more" style="font-size: 12px; font-family: 'Roboto', Arial, sans-serif; color: #7AC142; font-weight: bold; text-decoration: none; letter-spacing: 1.5px; margin-top:5px; margin-bottom:5px;" OnMouseOver="this.style.textDecoration='underline'; this.style.color='#7ac142'" OnMouseOut="this.style.textDecoration='none';this.style.color='#7ac142'"> 
								<xsl:attribute name="href"><xsl:value-of select="url"/></xsl:attribute> 
								<xsl:attribute name="target">_blank</xsl:attribute> 
								READ STORY > 
							</a></div> 

												</td> 
					</tr> 
				</table> 
			</xsl:template> 

		</xsl:stylesheet>